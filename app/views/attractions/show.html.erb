<h1><%= @attraction.name %></h1><br>
<ul>
<li>Tickets: <%= @attraction.tickets %></li><br>
<li>Nausea Rating: <%= @attraction.nausea_rating %></li><br>
<li>Happiness Rating:<%= @attraction.happiness_rating %></li><br>
<li>Minimum Height Required: <%= @attraction.min_height %></li><br>
</ul>
<% if !current_user.admin %>
<%= button_to "Go on this ride", ride_path(@attraction.id) %>
<% else %>
<%= link_to "Edit Attraction", edit_attraction_path(@attraction.id) %>
<% end %>