<h1><%= @user.name  %></h1>
<% if flash[:notice] %>
    <%= flash[:notice] %>
<% end %>

<p>Mood: <%= @user.mood if @user.mood %></p>
<p>Tickets: <%= @user.tickets %></p>
<p>Height: <%= @user.height %></p>
<p><%= @user.admin? %></p>
<%= link_to "See attractions", attractions_path %><br>
<%= link_to "Log Out", logout_path, method: "post" %>

